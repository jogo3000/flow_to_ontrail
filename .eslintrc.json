{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb-base", "prettier"],
    "globals": {
        "browser": true,
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": ["error", {"args": "after-used"}]
    }
}
